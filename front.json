{
    "name": "flip-app",
    "version": "0.0.1",
    "private": true,
    "scripts": {
      "install": "oss-license-check --html ./App/assets/htmls/oss_license.html --template ./App/assets/htmls/oss_license_template.html",
      "android": "react-native run-android --variant=debug",
      "ios": "react-native run-ios",
      "start": "react-native start",
      "test": "jest",
      "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
      "ios-bundle": "npm run generate-oss && react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ./ios --platform ios",
      "android-bundle": "npm run generate-oss && react-native bundle --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --platform android",
      "generate-oss": "oss-license-check --html ./App/assets/htmls/oss_license.html --template ./App/assets/htmls/oss_license_template.html",
      "devtools": "react-devtools"
    },
    "dependencies": {
